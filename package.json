{
  "author": "Jean-Paul S. Boodhoo",
  "name": "@developwithpassion/curry_js",
  "main": "./dist/index.js",
  "description": "Simple curry utility",
  "version": "0.0.1",
  "repository": {
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/register": "^7.8.3",
    "chai": "^4.2.0",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.2",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "mocha": "^6.2.2",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "bump-patch": "pnpm version patch",
    "build": "parcel build ./lib/index.js",
    "docs-html": "documentation build lib/index.js -f html -o docs",
    "pretty": "prettier --config-precedence prefer-file --write \"lib/**/*.js\"",
    "lint": "eslint lib/**/*.js",
    "test": "mocha --require @babel/register ./lib/**/*.spec.js"
  },
  "lint-staged": {
    "lib/**/*.js": [
      "pnpm run pretty",
      "pnpm run lint",
      "pnpm run build",
      "pnpm run docs-html",
      "git add"
    ]
  }
}
